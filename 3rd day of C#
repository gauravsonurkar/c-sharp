

//Switch program 
using System;
class switchprg
{
  public static void Main ()
  {
    Console.WriteLine ("Enter the number : ");
    int num = Convert.ToInt32 (Console.ReadLine ());

    switch (num)
      {
      case 10:
	Console.WriteLine ("It is 10 ");
	break;
	case 20:Console.WriteLine ("It is 20 ");
	break;
	case 30:Console.WriteLine ("It is 30 ");
	break;

	default:Console.WriteLine ("enter the wrong number ....");
	break;


      }
  }
}

//Simple for loop
using System;
class forprogram
{
public static void Main()
{
    int n = 10;
    
    for(int i = 1; i<=n ; i++)
    {
        Console.WriteLine(i);
    }
    
}

}

// outer and inner loop

using System;
class forprogram
{
public static void Main()
{
    for(int i = 1; i <= 3 ; i++)
    {
        for(int j = 1 ;j<=3;j++)
        {
            Console.WriteLine(i+ " " +j);
        }
    }
    
}

}

