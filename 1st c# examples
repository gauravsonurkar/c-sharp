// }
    
//Using namespace
// We create a classes inside the namespace , it is used to group related classes it is 
//used to categorized classes easy to maintain

using System;
namespace consoleaplication
{
    public class program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Hello World");
        }
    }
}

